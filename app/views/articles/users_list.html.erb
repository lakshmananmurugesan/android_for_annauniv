<h1>Users List</h1>
<div id="articles">
  <table>
    <thead>
    <tr>
      <th>Users</th>
      <th>Phone</th>
      <th>Email</th>
    </tr>
    </thead>

    <tbody>
    <% @userlist.each do |user| %>
        <tr>
          <td><%= user.name%></td>
          <td><%= user.phone%></td>
          <td><%= user.email%></td>
        </tr>
    <% end %>
    </tbody>
  </table>
</div>